<script>
    function showPrices(type) {
        if (type === 'restaurant') {
            document.getElementById('restaurant-prices').style.display = 'block';
            document.getElementById('franchise-prices').style.display = 'none';
        } else if (type === 'franchise') {
            document.getElementById('restaurant-prices').style.display = 'none';
            document.getElementById('franchise-prices').style.display = 'block';
        }
        // Establecer el bot√≥n activo
        document.querySelectorAll('.tabs a').forEach(item => {
            item.classList.remove('active');
        });
        document.querySelector(`.tabs a[href="#"][onclick="showPrices('${type}']`).classList.add('active');
    }
    // Llamar a la funci√≥n showPrices al cargar la p√°gina
    window.onload = function () {
        showPrices('restaurant');
    };
</script>
<script>
    //this script is for that the user not have most free pack
    // Obtener el bot√≥n por su ID
    const button = document.getElementById('checkout-and-portal-button');

    // Obtener el valor del atributo 'status' del bot√≥n
    const statusValue = button.getAttribute('status');

    // Verificar si el valor del status es diferente de 0
    if (statusValue !== '0') {
        button.remove();
    }
</script>