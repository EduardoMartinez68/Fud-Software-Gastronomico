<!--this script will be that the description of the web choose depending in where is the user-->
<script>
    var url = window.location.href;
    var works = url.split("/");
    var description = works.pop();
    document.getElementById("des").textContent = text_convert(description);

    function text_convert(title) {
        var textConvert = title.replace(/-/g, ' ');
        if (textConvert==''){
            return 'Füd'
        }
        return textConvert;
    }
</script>

<!--this script is for the wallpaper black-->
<script>
// Function to toggle the 'active' class based on checkbox state
function wallpaper_active() {
    // Get the checkbox element by its ID
    var checkbox = document.getElementById("check");
    
    // Get the div element by its ID
    var wallpaperNavbar = document.getElementById("wallpaper-navbar");

    // Toggle the 'active' class based on checkbox state
    if (checkbox.checked) {
        wallpaperNavbar.classList.add("active");
    } else {
        wallpaperNavbar.classList.remove("active");
    }
}
</script>